{
  "manifest_version": 3,
  "name": "Nocred",
  "version": "1.0",
  "icons": {
    "16": "./assets/images/icon-16.png",
    "32": "./assets/images/icon-32.png",
    "48": "./assets/images/icon-48.png",
    "128": "./assets/images/icon-128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "cookies", "tabs"],
  "host_permissions": ["<all_urls>"]
}
